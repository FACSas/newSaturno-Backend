{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Access-Control-Allow-Methods": "*",
  "AllowedHosts": "*",
  "headers": {
    "Access-Control-Allow-Origin": "*",
    "Access-Control-Allow-Methods": "GET,PUT,POST,DELETE,PATCH,OPTIONS"
  },
  "cors": {
    "rules": [
      {
        "origin": "*",
        "allow": true
      }
    ]
  }
}
